    <script type="text/javascript">
        var inactivityTimeout;

        function resetInactivityTimeout() {
            clearTimeout(inactivityTimeout);
            // Set the timeout to 5 minutes (300,000 milliseconds)
            inactivityTimeout = setTimeout(logout, 900000);
        }

        function logout() {
            // Redirect to the logout or login page
            window.location.href = '../login';
        }

        // Add event listeners for user activity (e.g., mousemove, keydown, etc.)
        document.addEventListener('mousemove', resetInactivityTimeout);
        document.addEventListener('keydown', resetInactivityTimeout);

        // Initialize the timeout on page load
        resetInactivityTimeout();
    </script>
   <script>
       function SuccessToastr(message, title) {
           setTimeout(function () {
               toastr.options = {
                   closeButton: true,
                   progressBar: true,
                   //positionClass: "toast-top-center",
                   showMethod: 'slideDown',
                   timeOut: 2000
               };
               //toastr.success(message, title);
               toastr.success(`<span style="font-size: 20px">${message}</span>`, title);
           }, 100);
       }

       function WarningToastr(message, title) {
           setTimeout(function () {
               toastr.options = {
                   closeButton: true,
                   progressBar: true,
                   //positionClass: "toast-top-center",
                   showMethod: 'slideDown',
                   timeOut: 2000


               };
               toastr.warning(`<span style="font-size: 20px">${message}</span>`, title);
           }, 100);
       }
   </script>

     <script type="text/javascript">

         function success(msg, redirectUrl) {

             toastr.option = {

                 "debug": false,

                 "positionClass": "toast-top-center",

                 "Onclick": null

             }

             toastr.success(msg, "Success");

             setTimeout(function () {
                 window.location.href = redirectUrl;
             }, 1000);

             return false;

         }


         function error(msg) {

             toastr.option = {

                 "debug": false,

                 "positionClass": "toast-top-center",

                 "Onclick": null

             }

             toastr.error(msg, "Error");

             return false;

         }

         function warning(msg) {

             toastr.option = {

                 "debug": false,

                 "positionClass": "toast-top-center",

                 "Onclick": null

             }

             toastr.warning(msg, "Warning");

             return false;

         }


     </script>
